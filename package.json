{
  "name": "essential-ts-utils",
  "author": "Daphne Smit",
  "description": "Essential Typescript utils is a collection of useful common used Javascript utilities written in Typescript.",
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/essential-ts-utils.esm.js",
  "homepage": "https://github.com/daphnesmit/essential-ts-utils#readme",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "utilities",
    "utils",
    "essential",
    "helpers",
    "array",
    "object",
    "json"
  ],
  "contributors": [
    {
      "name": "Daphne Smit",
      "email": "hi@daphnesmit.nl"
    },
    {
      "name": "Matt van Voorst",
      "email": "matt.vanvoorst@deptagency.nl"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daphnesmit/essential-ts-utils"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ]
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "commitmsg": "npx commitlint -e $GIT_PARAMS",
    "commit": "commit",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "commit-msg": "npm run commitmsg",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@types/jest": "^26.0.20",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
